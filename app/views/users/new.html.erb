<h1>Admin sign up.</h1>

<%= form_for :user, url: users_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email %>
  </p>
  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>		
  </p>
  <p>
    <%= f.submit "Sign Up" %>
  </p>
  
<% end %>